//Triangle or not
import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int n1=in.nextInt();
        int n2=in.nextInt();
        int n3=in.nextInt();
        if ((n1+n2+n3)==180){
            System.out.println("Triangle can be formed");
        }
        else{
            System.out.println("Triangle can not be formed");
        }
    }
}
